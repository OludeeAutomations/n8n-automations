    {
      "parameters": {
        "jsCode": "const keywords = [\"n8n\", \"typescript\", \"remote nigeria\"]; // â† customize this list\n\nreturn items.filter(item => {\n  const job = item.json;\n\n  const position = (job.position || \"\").toLowerCase();\n  const tags = (job.tags || []).map(tag => tag.toLowerCase());\n  const company = (job.company || \"\").toLowerCase();\n\n  // Combine all fields into one searchable string\n  const combinedText = [position, ...tags, company].join(\" \");\n\n  // Check if any keyword is found\n  return keywords.some(keyword => combinedText.includes(keyword));\n});\n"
      },
